<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.oscarehr.eyeform.model">
	<class
		name="FollowUp"
		table="eyeform_followup"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="native"/>
		</id>

		<property
			name="appointmentNo"
			column="appointment_no"
			type="integer"
			not-null="false"
		/>
		
		<property
			name="demographicNo"
			column="demographic_no"
			type="integer"
			not-null="false"
		/>
<!-- 		
		<many-to-one
			name="demographic"
			column="demographic_no"
			class="org.oscarehr.common.model.Demographic"
			not-null="false"
		>
		</many-to-one>
-->
		
		<property
			name="timespan"
			column="timespan"
			type="integer"
			not-null="false"			
		/>

		<property
			name="timeframe"
			column="timeframe"
			type="string"
			not-null="false"			
		/>
		
		<property
			name="followupProvider"
			column="followup_provider"
			type="string"
			not-null="false"			
		/>
		
		<!-- list of "templates" -->		
		
		<property
			name="date"
			column="date"
			type="timestamp"
			not-null="false"
			length="19"
		/>


		<many-to-one name="provider" class="org.oscarehr.common.model.Provider"
                        column="followup_provider" update="false" insert="false" lazy="false" />
                        
	</class>	
</hibernate-mapping>